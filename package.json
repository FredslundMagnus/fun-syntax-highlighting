{
    "name": "fun",
    "displayName": "Funny Language Support",
    "description": "Soppurt for the Funny programmin language.",
    "version": "0.1.0",
    "publisher": "Magnus Fredslund",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/FredslundMagnus/fun-syntax-highlighting.git"
    },
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "Funny",
                "aliases": [
                    "Funny",
                    "fun",
                    "Fun",
                    "Fun-lang"
                ],
                "extensions": [
                    ".fun"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "Funny",
                "scopeName": "source.fun",
                "path": "./syntaxes/fun.tmLanguage.json"
            }
        ]
    }
}